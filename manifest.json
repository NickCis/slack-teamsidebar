{
  "name": "Slack team sidebar",
  "version": "1.0.0",
  "description": "Adds the team sidebar on the Slack web client",
  "homepage_url": "https://github.com/NickCis/slack-teamsidebar/",
  "manifest_version": 2,
  "background": {},
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png",
    "512": "icons/icon-512.png"
  },
  "content_scripts": [
    {
      "matches": ["https://app.slack.com/client/*"],
      "js": ["./content-script.js"]
    }
  ]
}
